 0.0 + 0.0 * C.xxx;\n  //   x1 = x0 - i1  + 1.0 * C.xxx;\n  //   x2 = x0 - i2  + 2.0 * C.xxx;\n  //   x3 = x0 - 1.0 + 3.0 * C.xxx;\n  vec3 x1 = x0 - i1 + C.xxx;\n  vec3 x2 = x0 - i2 + C.yyy; // 2.0*C.x = 1/3 = C.y\n  vec3 x3 = x0 - D.yyy;      // -1.0+3.0*C.x = -0.5 = -D.y\n\n// Permutations\n  i = mod289(i);\n  vec4 p = permute( permute( permute(\n             i.z + vec4(0.0, i1.z, i2.z, 1.0 ))\n           + i.y + vec4(0.0, i1.y, i2.y, 1.0 ))\n           + i.x + vec4(0.0, i1.x, i2.x, 1.0 ));\n\n// Gradients: 7x7 points over a square, mapped onto an octahedron.\n// The ring size 17*17 = 289 is close to a multiple of 49 (49*6 = 294)\n  float n_ = 0.142857142857; // 1.0/7.0\n  vec3  ns = n_ * D.wyz - D.xzx;\n\n  vec4 j = p - 49.0 * floor(p * ns.z * ns.z);  //  mod(p,7*7)\n\n  vec4 x_ = floor(j * ns.z);\n  vec4 y_ = floor(j - 7.0 * x_ );    // mod(j,N)\n\n  vec4 x = x_ *ns.x + ns.yyyy;\n  vec4 y = y_ *ns.x + ns.yyyy;\n  vec4 h = 1.0 - abs(x) - abs(y);\n\n  vec4 b0 = vec4( x.xy, y.xy );\n  vec4 b1 = vec4( x.zw, y.zw );\n\n  //vec4 s0 = vec4(lessThan(b0,0.0))*2.0 - 1.0;\n  //vec4 s1 = vec4(lessThan(b1,0.0))*2.0 - 1.0;\n  vec4 s0 = floor(b0)*2.0 + 1.0;\n  vec4 s1 = floor(b1)*2.0 + 1.0;\n  vec4 sh = -step(h, vec4(0.0));\n\n  vec4 a0 = b0.xzyw + s0.xzyw*sh.xxyy ;\n  vec4 a1 = b1.xzyw + s1.xzyw*sh.zzww ;\n\n  vec3 p0 = vec3(a0.xy,h.x);\n  vec3 p1 = vec3(a0.zw,h.y);\n  vec3 p2 = vec3(a1.xy,h.z);\n  vec3 p3 = vec3(a1.zw,h.w);\n\n//Normalise gradients\n  vec4 norm = taylorInvSqrt(vec4(dot(p0,p0), dot(p1,p1), dot(p2, p2), dot(p3,p3)));\n  p0 *= norm.x;\n  p1 *= norm.y;\n  p2 *= norm.z;\n  p3 *= norm.w;\n\n// Mix final noise value\n  vec4 m = max(0.6 - vec4(dot(x0,x0), dot(x1,x1), dot(x2,x2), dot(x3,x3)), 0.0);\n  m = m * m;\n  return 42.0 * dot( m*m, vec4( dot(p0,x0), dot(p1,x1),\n                                dot(p2,x2), dot(p3,x3) ) );\n  }\n\nvarying vec2 vUv;\nuniform float u_radius;\nuniform bool u_theme_dark;\nuniform float u_intensity;\nuniform float u_blur;\nuniform vec2 u_resolution;\nuniform vec2 u_mouse;\nuniform sampler2D u_noise_1;\nuniform sampler2D u_noise_2;\nuniform sampler2D uTexture;\nuniform sampler2D uTextureDark;\nuniform sampler2D tDiffuse;\n\nfloat blendOverlay( float base, float blend ) {\n    return( base < 0.5 ? ( 2.0 * base * blend ) : ( 1.0 - 2.0 * ( 1.0 - base ) * ( 1.0 - blend ) ) );\n}\n\nvec3 blendOverlay( vec3 base, vec3 blend ) {\n    return vec3( blendOverlay( base.r, blend.r ), blendOverlay( base.g, blend.g ), blendOverlay( base.b, blend.b ) );\n}\n\nfloat circle(in vec2 _st, in float _radius, in float blurriness){\n    vec2 dist = _st;\n    return 1. - smoothstep(_radius - (_radius*blurriness), _radius + (_radius * blurriness), dot(dist, dist) * 4.0);\n}\n\nvoid main() {\n    vec4 previousPassColor = texture2D(tDiffuse, vUv);\n    vec4 texture = texture2D(uTexture, vUv);\n    vec4 textureDark = texture2D(uTextureDark, vUv);\n\n    vec4 greenScreen = vec4(0.,1.,0.,1.);\n    vec3 diff = previousPassColor.rgb - greenScreen.rgb;\n    float fac = smoothstep(0.55-0.05,0.55+0.05, dot( diff, diff));\n\n    vec4 normalPass = mix(previousPassColor, vec4(0.,0.,0.,0.), 1. - fac);\n    vec3 invertedPass = vec3(1.0) - normalPass.rgb;\n\n    invertedPass.rgb *= .7;\n\n    if(previousPassColor.a < .5) {\n        invertedPass = previousPassColor.rgb;\n    }\n\n    vec3 finalColor = vec3(min(normalPass.rgb, texture.rgb));\n    vec3 finalColorDark = vec3(max(invertedPass.rgb, textureDark.rgb));\n\n    // finalColorDark.rgb *= .8;\n\n    if(u_radius > 0.001) {\n        vec2 mouse = u_mouse * -0.5;\n        vec2 st = gl_FragCoord.xy / u_resolution.xy - vec2(0.5);\n\n        st.y *= u_resolution.y / u_resolution.x;\n        mouse.y *= u_resolution.y / u_resolution.x;\n\n        vec2 circlePos = st + mouse;\n        float c = circle(circlePos, u_radius, u_blur) * u_intensity;\n        float offx = vUv.x + sin(vUv.y);\n        float offy = vUv.y;\n\n        vec2 tile = gl_FragCoord.xy / u_resolution;\n        vec2 tileBig = tile * 1.25;\n        vec2 tileSmall = tile * 3.25;\n\n        float noise = snoise(vec3(offx, offy, 0.) * 4.) - 1.;\n        vec4 noiseTexture = texture2D(u_noise_1, tileBig);\n        vec4 noiseTextureSmall = texture2D(u_noise_2, tileSmall);\n        float finalMask = smoothstep(0.0,1.0, noise * noiseTexture.r * noiseTextureSmall.r + c);\n\n        vec3 mixed;\n\n        if(u_theme_dark == true) {\n            mixed = mix(finalColorDark.rgb, finalColor.rgb, finalMask);\n        } else {\n            mixed = mix(finalColor.rgb, finalColorDark.rgb, finalMask);\n        }\n\n        // mixed.rgb -= 0.03;\n\n        gl_FragColor = vec4(mixed.rgb, normalPass.a);\n    } else {\n        vec4 mixed;\n\n        if(u_theme_dark == true) {\n            mixed = vec4(finalColorDark, normalPass.a);\n        } else {\n            mixed = vec4(finalColor, normalPass.a);\n        }\n\n        // mixed.rgb -= 0.03;\n\n        gl_FragColor = mixed;\n    }\n}")},S={uniforms:{tDiffuse:{value:null},u_time:{value:0},u_radius:{value:0},u_intensity:{value:2.6},u_blur:{value:1},u_noise_1:{value:null},u_noise_2:{value:null},u_theme_neon:{value:!1},u_color_2:{value:new a.d(x.d.colors.neon)},u_color_1:{value:new a.d("#000000")},u_mouse:{value:new a.A(0,0)},u_resolution:{value:new a.A(x.d.layout.w,x.d.layout.h)}},vertexShader:_()(g),fragmentShader:_()("#define GLSLIFY 1\n//\n// Description : Array and textureless GLSL 2D/3D/4D simplex\n//               noise functions.\n//      Author : Ian McEwan, Ashima Arts.\n//  Maintainer : ijm\n//     Lastmod : 20110822 (ijm)\n//     License : Copyright (C) 2011 Ashima Arts. All rights reserved.\n//               Distributed under the MIT License. See LICENSE file.\n//               https://github.com/ashima/webgl-noise\n//\n\nvec3 mod289(vec3 x) {\n  return x - floor(x * (1.0 / 289.0)) * 289.0;\n}\n\nvec4 mod289(vec4 x) {\n  return x - floor(x * (1.0 / 289.0)) * 289.0;\n}\n\nvec4 permute(vec4 x) {\n     return mod289(((x*34.0)+1.0)*x);\n}\n\nvec4 taylo